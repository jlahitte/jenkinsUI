<ul>
{{#each job.builds}}
	{{#if changes}}
		<li  class={{success}}><div class="buildDetailList"><a href="#" onclick="togglechangeSetDetail(event,'env-{{../../instance_name}}_build-{{number}}');" class="buildDetailLink">Build n°{{number}} du {{buildDate}} - Voir les changements</a></div></li>
		<div id='env-{{../../instance_name}}_build-{{number}}' data-buildNumber="{{number}}" class="buildDetail">
			<div class="commitsText">
				<ul>
					{{#each buildDetail.changeSet.items}}
						<li>{{date}} - {{msg}}</li>
						<span> {{comment}}</span>
					{{/each}}
				</ul>
			</div>
		</div>
	{{else}}
		<li  class={{success}}><div class="buildDetailList">Build n°{{number}} du {{buildDate}} - sans modification</div></li>			
	{{/if}}
{{/each}}
</ul>